{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.3",
        "ext-json": "*",
        "aws/aws-sdk-php-laravel": "~3.0",
        "bacon/bacon-qr-code": "^1.0",
        "barryvdh/laravel-snappy": "^0.4.8",
        "cohensive/embed": "^5.5",
        "facade/ignition": "^2.3.6",
        "fideloper/proxy": "^4.0",
        "fruitcake/laravel-cors": "^3.0",
        "goodby/csv": "^1.3",
        "google/analytics-admin": "^0.14.0",
        "google/apiclient": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "joe1992w/laravel-media-embed": "^1.0",
        "laravel/framework": "^8.0",
        "laravel/horizon": "^5.14",
        "laravel/passport": "^10",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^3.0",
        "laravelcollective/html": "^6.2",
        "league/flysystem-aws-s3-v3": "^1.0",
        "maatwebsite/excel": "^3.1",
        "mailchimp/transactional": "^1.0",
        "nodes/nemid": "dev-master",
        "nunomaduro/collision": "^5.0",
        "omniphx/forrest": "^2.11",
        "opentok/opentok": "dev-main",
        "paragonie/random_compat": "2.*",
        "pelago/emogrifier": "^5.0",
        "predis/predis": "^1.1",
        "quickpay/quickpay-php-client": "1.0.*",
        "stripe/stripe-php": "^7.104",
        "vemcogroup/laravel-sparkpost-driver": "^4.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.5",
        "barryvdh/laravel-ide-helper": "^2.9",
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "laravel/sail": "^1.4",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "^9.0",
        "symfony/thanks": "^1.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories",
            "app/Helpers"
        ],
        "psr-4": {
            "App\\": "app/"
        },
		"files": [
			"app/Helpers/Helper.php",
            "app/Helpers/EventHelper.php",
            "app/Helpers/UploadHelper.php",
            "app/Helpers/SmsHelper.php",
            "app/Helpers/AWS/S3/S3.php",
            "app/Helpers/LeadHelper.php"
		]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/dusk"
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "github-oauth": {
        },
        "allow-plugins": {
            "symfony/thanks": true
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/eventbuizz/nemid"
        },
        {
            "type": "vcs",
            "url": "https://github.com/eventbuizz/OpenTok-PHP-SDK"
        }
    ]
}
